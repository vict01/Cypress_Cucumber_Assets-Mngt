{
    "proxy": "http://localhost:5000",
    "name": "assets-management-case",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "classnames": "^2.2.6",
        "clone": "^2.1.1",
        "cypress": "^9.7.0",
        "@testing-library/cypress": "^8.0.3",
        "cypress-cucumber-preprocessor": "4.3.1",
        "multiple-cucumber-html-reporter": "^1.21.4",
        "gh-pages": "^1.1.0",
        "lint-staged": "^6.0.1",
        "mdbreact": "^4.27.0",
        "prettier": "^1.10.2",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-jsonschema-form": "^1.0.0",
        "react-markdown": "^3.1.4",
        "react-router-dom": "^4.2.2",
        "react-scripts": "1.0.17",
        "recompose": "^0.26.0",
        "semantic-ui-css": "^2.2.12",
        "semantic-ui-react": "^0.77.2",
        "uuid": "^8.3.1"
    },
    "cypress-cucumber-preprocessor": {
        "nonGlobalStepDefinitions": true,
        "cucumberJson": {
            "generate": true,
            "outputFolder": "cypress/cucumber-json",
            "filePrefix": "",
            "fileSuffix": ".cucumber"
        }
    },
    "scripts": {
        "cy:run": "cypress run --env TAGS=\"not @skip\" --browser chrome --headed && npm run cucumber-report",
        "cucumber-report": "npm run clean-old-report && node cucumber-html-report.js",
        "clean-old-report": "del reports\\cucumber-htmlreport.html\\index.html",
        "start-api": "cd api && set FLASK_APP=app.py && python -m flask run",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "homepage": "http://localhost:3000/#"
}